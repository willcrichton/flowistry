{
  "name": "flowistry",
  "publisher": "wcrichton",
  "displayName": "Flowistry",
  "repository": "https://github.com/willcrichton/flowistry",
  "description": "",
  "version": "0.3.2",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:rust"
  ],
  "main": "./out/extension.js",
  "files": [
    "out",
    "src"
  ],
  "contributes": {
    "commands": [
      {
        "command": "flowistry.forward_select",
        "title": "Flowistry: Forward Select"
      },
      {
        "command": "flowistry.forward_highlight",
        "title": "Flowistry: Forward Highlight"
      },
      {
        "command": "flowistry.backward_select",
        "title": "Flowistry: Backward Select"
      },
      {
        "command": "flowistry.backward_highlight",
        "title": "Flowistry: Backward Highlight"
      },
      {
        "command": "flowistry.effects",
        "title": "Flowistry: Effects"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "group": "navigation@10",
          "submenu": "flowistry"
        }
      ],
      "flowistry": [
        {
          "command": "flowistry.backward_highlight",
          "when": "editorHasSelection"
        },
        {
          "command": "flowistry.backward_select",
          "when": "editorHasSelection"
        },
        {
          "command": "flowistry.forward_highlight",
          "when": "editorHasSelection"
        },
        {
          "command": "flowistry.forward_select",
          "when": "editorHasSelection"
        },
        {
          "command": "flowistry.effects"
        }
      ]
    },
    "submenus": [
      {
        "id": "flowistry",
        "label": "Flowistry"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "node build.cjs",
    "watch": "node build.cjs -w"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@wcrichto/esbuild-utils": "^0.3.0",
    "esbuild": "^0.12.24",
    "esbuild-sass-plugin": "^1.5.2",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "__metadata": {
    "id": "2551f9f8-fbd2-43e8-8490-26056fdfb26f",
    "publisherDisplayName": "Will Crichton",
    "publisherId": "1026bfbe-4ffd-44f9-ae41-e620853f9572"
  },
  "dependencies": {
    "@types/lodash": "^4.14.172",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
