{
  "name": "flowistry",
  "publisher": "wcrichton",
  "license": "MIT",
  "icon": "flowistry-logo-128.png",
  "displayName": "Flowistry",
  "galleryBanner": {
    "color": "white"
  },
  "repository": {
    "url": "https://github.com/willcrichton/flowistry",
    "type": "git"
  },
  "description": "Information Flow in the IDE for Rust",
  "version": "0.5.25",
  "engines": {
    "vscode": "^1.54.0"
  },
  "bugs": {
    "url": "https://github.com/willcrichton/flowistry/issues",
    "email": "wcrichto@cs.stanford.edu"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:rust"
  ],
  "main": "./out/extension.js",
  "files": [
    "out"
  ],
  "contributes": {
    "commands": [
      {
        "command": "flowistry.focus",
        "title": "Flowistry: Toggle focus mode"
      },
      {
        "command": "flowistry.focus_mark",
        "title": "Flowistry: Set focus mark"
      },
      {
        "command": "flowistry.focus_unmark",
        "title": "Flowistry: Remove focus mark"
      },
      {
        "command": "flowistry.focus_select",
        "title": "Flowistry: Select focused region"
      },
      {
        "command": "flowistry.decompose",
        "title": "Flowistry: Decompose"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "group": "navigation@10",
          "submenu": "flowistry"
        }
      ],
      "flowistry": [
        {
          "command": "flowistry.focus"
        },
        {
          "command": "flowistry.focus_mark"
        },
        {
          "command": "flowistry.focus_unmark"
        },
        {
          "command": "flowistry.focus_select"
        }
      ]
    },
    "submenus": [
      {
        "id": "flowistry",
        "label": "Flowistry"
      }
    ],
    "keybindings": [
      {
        "command": "flowistry.focus",
        "key": "ctrl+r ctrl+a",
        "mac": "meta+r meta+a"
      },
      {
        "command": "flowistry.focus_mark",
        "key": "ctrl+r ctrl+s",
        "mac": "meta+r meta+s"
      },
      {
        "command": "flowistry.focus_unmark",
        "key": "ctrl+r ctrl+d",
        "mac": "meta+r meta+d"
      },
      {
        "command": "flowistry.focus_select",
        "key": "ctrl+r ctrl+t",
        "mac": "meta+r meta+t"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "node build.cjs",
    "watch": "node build.cjs -w",
    "tc": "tsc",
    "lint": "eslint src/*.ts",
    "pretest": "tsc --outDir test && npm run build",
    "fmt": "prettier -w src/*.ts",
    "test": "node ./test/tests/runTests.js"
  },
  "devDependencies": {
    "@flatten-js/interval-tree": "^1.0.16",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/adm-zip": "^0.4.34",
    "@types/chai": "^4.2.22",
    "@types/deep-equal-in-any-order": "^1.0.1",
    "@types/glob": "^7.1.4",
    "@types/got": "^9.6.12",
    "@types/lodash": "^4.14.172",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.20.46",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@wcrichto/esbuild-utils": "^0.3.0",
    "chai": "^4.3.4",
    "deep-equal-in-any-order": "^1.1.15",
    "esbuild-sass-plugin": "^1.5.2",
    "eslint": "^8.9.0",
    "estrella": "^1.4.1",
    "minimist": "^1.2.5",
    "mocha": "^9.1.2",
    "mockttp": "^2.4.0",
    "prettier": "^2.5.1",
    "toml": "^3.0.0",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "wait-for-expect": "^3.0.2"
  },
  "__metadata": {
    "id": "2551f9f8-fbd2-43e8-8490-26056fdfb26f",
    "publisherDisplayName": "Will Crichton",
    "publisherId": "1026bfbe-4ffd-44f9-ae41-e620853f9572"
  },
  "dependencies": {
    "adm-zip": "^0.5.8",
    "classnames": "^2.3.1",
    "got": "^11.8.2",
    "lodash": "^4.17.21",
    "new-github-issue-url": "^0.2.1",
    "open": "^8.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
